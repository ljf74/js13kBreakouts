{
  "name": "kontra",
  "version": "1.0.0",
  "description": "KontraJs Breakout Example",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "node build.js",
    "clean": "rm -rf dist",
    "prepare": "mkdir dist && cp logo.png dist",
    "pack": "npm run clean && npm run prepare && js13k-packer index.html dist -o js13kBreakout.zip && cp dist/js13kBreakout.zip . && npm run clean"
  },
  "author": "Steven Lambert",
  "devDependencies": {
    "esbuild": "^0.14.51",
    "esbuild-plugin-kontra": "^1.0.0",
    "js13k-packer": "^1.0.2"
  }
}
